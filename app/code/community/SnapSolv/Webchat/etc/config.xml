<config>
    <modules>
        <SnapSolv_Webchat>
            <version>0.0.1</version>
        </SnapSolv_Webchat>
    </modules>
    <global>
		<models>
            <webchat>
                <class>SnapSolv_Webchat_Model</class>
            </webchat>     
        </models>
        <helpers>
            <webchat>
                <class>SnapSolv_Webchat_Helper</class>
            </webchat>
        </helpers>
    </global>
	<frontend>
		<layout>
            <updates>
                <SnapSolv_Webchat module="SnapSolv_Webchat">
                    <file>snapsolv_webchat.xml</file>
                </SnapSolv_Webchat>
            </updates>
        </layout>
	</frontend>
    <adminhtml>
        <layout>
            <updates>
                <basket>
                    <file>picker.xml</file>
                </basket>
            </updates>
        </layout>
    </adminhtml>
</config>
